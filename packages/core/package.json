{
  "name": "@montflow/core",
  "version": "1.0.1",
  "author": "Daniel Montilla",
  "description": "A collection of powerful, lightweight and performant typescript utilities.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/montflow/montflow.git"
